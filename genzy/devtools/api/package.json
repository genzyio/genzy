{
  "name": "@genzy.io/devtools-api",
  "version": "0.0.1-alpha-4",
  "description": "Genzy Devtools API.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc && node dist",
    "start": "npm run build:client && tsc && npm run copy-to-dist && node dist",
    "build": "npm run build:client && tsc --declaration && npm run copy-to-dist",
    "build:client": "cd ../client && npm run build && rm -rf ../api/public 2> /dev/null && cp -r dist ../api/public",
    "copy-to-dist": "rm -rf ./dist/public 2> /dev/null && cp -r public dist/public",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "genzy",
    "api",
    "documentation"
  ],
  "author": "@genzy.io",
  "license": "ISC",
  "dependencies": {
    "axios": "1.5.1",
    "@genzy.io/cli": "0.0.1-alpha",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "find-pid-from-port": "0.1.0",
    "moment": "2.29.4",
    "multer": "1.4.5-lts.1",
    "sqlite3": "5.1.6",
    "terminate": "^2.6.1"
  },
  "devDependencies": {
    "@jest/globals": "29.0.3",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.14",
    "@types/multer": "1.4.7",
    "jest": "29.0.3",
    "ts-jest": "29.0.3",
    "typescript": "4.9.3"
  }
}