{
  "name": "n1mbly-redis-plugin",
  "version": "0.0.4",
  "main": "build/index.js",
  "description": "N1mbly plugin for connecting to Redis and injecting a Redis service.",
  "types": "build/index.d.ts",
  "scripts": {
    "prepublish": "([ -e build ] && rm -r build || [ -e . ]) && npm run build && cp src/index.d.ts build",
    "test": "jest",
    "build": "webpack"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "bjelicaluka",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/jest": "27.0.3",
    "@types/supertest": "2.0.11",
    "esbuild-loader": "2.16.0",
    "jest": "27.3.1",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "typescript": "4.5.2",
    "webpack": "5.64.2",
    "webpack-cli": "4.9.1"
  },
  "dependencies": {
    "@n1mbly/api": "0.2.10"
  },
  "gn1mblyMetadata": {
    "microservices": [
      {
        "id": "Redis{{installingOnMicroserviceId}}",
        "name": "Redis",
        "showName": true,
        "type": "imageNode",
        "url": "https://w7.pngwing.com/pngs/230/99/png-transparent-redis-original-wordmark-logo-icon.png",
        "width": 150,
        "height": 150,
        "services": [
          {
            "id": "Redis{{installingOnMicroserviceId}}_RedisService",
            "name": "RedisService",
            "functions": [
              {
                "id": "Redis_Get",
                "name": "get",
                "returnType": "string"
              },
              {
                "id": "Redis_Set",
                "name": "set",
                "returnType": "string"
              },
              {
                "id": "RedisDelete",
                "name": "delete",
                "returnType": "string"
              }
            ]
          }
        ],
        "dependencies": []
      }
    ],
    "communications": [
      {
        "source": "{{installingOnMicroserviceId}}",
        "target": "Redis{{installingOnMicroserviceId}}"
      }
    ]
  }
}
